<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

 <title>tomerfiliba.com | RPyC Development Blog</title>
 <link href="https://www.tomerfiliba.com/blog/rpyc-atom.xml" rel="self"/>
 <link href="https://www.tomerfiliba.com/rpyc-blog"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>https://www.tomerfiliba.com/rpyc-blog</id>
 <author>
   <name>Tomer Filiba</name>
   <email>tomerfiliba@gmail.com</email>
 </author>

 {% for post in site.categories.blog limit:50 %}
 {% if post.tags contains "rpyc" %}
 {% unless post.draft %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="https://www.tomerfiliba.com{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>https://www.tomerfiliba.com{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endunless %}
 {% endif %}
 {% endfor %}

</feed>
